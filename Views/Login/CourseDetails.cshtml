@model List<System.String>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Subject</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
          Course Name:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model[0])
        </dd>
        <dt class = "col-sm-2">
           Start Date:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model[1])
        </dd>
        <dt class = "col-sm-2">
